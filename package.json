{
  "name": "enterprise-angular-generator",
  "version": "0.0.1",
  "description": "A collection of angular generators for enterprise applications.",
  "scripts": {
    "clean": "npm rm -rf dist",
    "clean-generated": "npm rm -rf src/*.ngfactory.ts && npm rm -rf src/*.ngsummary.json",
    "preparePackage": "node ./scripts/copy-package",
    "build-es2017": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine **/*_spec.js",
    "build": "npm run clean && npm run build-es2017 && npm run preparePackage && npm run clean-generated",
    "publishPackage": "npm run build && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cambronjay/enterprise-angular-generator.git"
  },
  "keywords": [
    "ngrx",
    "lazy loading",
    "angular",
    "generator",
    "schematics"
  ],
  "author": {
    "name": "Jay Cambron",
    "email": "cambronjay@gmail.com",
    "url": "http://jaycambron.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cambronjay/enterprise-angular-generator/issues"
  },
  "homepage": "https://github.com/cambronjay/enterprise-angular-generators#readme",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^0.0.22",
    "@angular-devkit/schematics": "^0.0.42",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.31",
    "jasmine": "^2.8.0",
    "rxjs": "^5.5.2",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "canonical-path": "0.0.2",
    "cpr": "3.0.1",
    "dgeni": "0.4.9",
    "copy-files": "0.1.0",
    "dgeni-packages": "0.19.0",
    "rimraf": "2.6.1",
    "semver": "5.3.0",
    "tslint": "5.2.0",
    "typescript": "2.3.2",
    "rxjs": "5.1.1"
  }
}
